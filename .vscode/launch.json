  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Django runserver",
      "type": "debugpy",
      "request": "launch",
      "args": ["runserver"],
      "django": true,
      "autoStartBrowser": false,
      "program": "${workspaceFolder}/manage.py"
    },
    {
      "name": "Python Debugger: Django test",
      "type": "debugpy",
      "request": "launch",
      "args": ["test"],
      "django": true,
      "autoStartBrowser": false,
      "program": "${workspaceFolder}/manage.py"
    },
    {
      "name": "Python Debugger: Collect Now",
      "type": "debugpy",
      "request": "launch",
      "args": ["collectnow"],
      "django": true,
      "autoStartBrowser": false,
      "program": "${workspaceFolder}/manage.py"
    }
  ]
}
